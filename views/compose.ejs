<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post" class="form-group">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" name="postTitle" class="form-control" />
    <label for="post">Post</label>
    <textarea
      type="text"
      name="postBody"
      id=""
      class="form-control"
      rows="3"
    ></textarea>
  </div>
  <button type="submit" class="btn btn-primary mb-2">Publish</button>
</form>

<%- include('partials/footer'); -%>
